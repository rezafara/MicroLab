#define CMD_GLCDON    0X3F
#define CMD_PAGE      0X68
#define CMD_YLINE     0X40
#define CMD_STARTLINE 0XC0

#define GLCD_DI  (1 << 7)
#define GLCD_RW  (1 << 6)
#define GLCD_E   (1 << 5)
#define GLCD_DB0 (1 << 0)
#define GLCD_DB1 (1 << 1)
#define GLCD_DB2 (1 << 2)
#define GLCD_DB3 (1 << 3)
#define GLCD_DB4 (1 << 4)
#define GLCD_DB5 (1 << 5)
#define GLCD_DB6 (1 << 6)
#define GLCD_DB7 (1 << 7)
#define GLCD_CS1 (1 << 4)
#define GLCD_CS2 (1 << 3)
#define GLCD_RST (1 << 2)

#define GLCD_DB_PORT PORTC
#define GLCD_SIG_PORT PORTA

void write_Cmd(unsigned char cmd);
void write_Data(unsigned char data,int cs);
void clearScreen();
void GLCDInit();
void gotoPage(int page,int y);

